'.source.fire':
  'editor':
    'autoIndentOnPaste': false
    'softTabs': true
    'tabLength': 4
    'commentStart': '# '
    # 'foldEndPattern': '^\\s*[}\\])]'
    'increaseIndentPattern': '^\\s*(class|func|elif|else|for|if|try|while|async|gen\\b.*\\s*$'
    'decreaseIndentPattern': '^\\s*(elif|else|catch)\\b.*\\s*$'
